{
  "name": "planechase",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --web",
    "wrangler:deploy": "wrangler pages deploy",
    "deploy": "set NODE_OPTIONS=--openssl-legacy-provider && npx expo export:web && npm run wrangler:deploy",
    "scryfall": "node ./util/update.js"
  },
  "dependencies": {
    "@expo/webpack-config": "^0.12.52",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-picker/picker": "2.4.10",
    "expo": "^49.0.21",
    "expo-checkbox": "~2.4.0",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.4.0",
    "expo-screen-orientation": "~6.0.6",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-web": "~0.18.10",
    "webpack-dev-server": "^3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3",
    "wrangler": "^3.78.9"
  },
  "private": true
}
